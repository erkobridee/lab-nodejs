      <option name="JS_STATEMENT" value="true" />
    </context>
  </template>
  <template name="rload" value="define(function(require) {&#10;  'use strict';&#10;&#10;  $END$&#10;&#10;});" description="require.js load modules" toReformat="true" toShortenFQNames="true">
